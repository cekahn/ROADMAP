        <rdfs:label xml:lang="en">medical record number</rdfs:label>
        <skos:altLabel xml:lang="en">MRN</skos:altLabel>
    </owl:Class>
    


    <!-- https://rsna.org/roadmap/ROADMAP_03562 -->

    <owl:Class rdf:about="https://rsna.org/roadmap/ROADMAP_03562">
        <rdfs:subClassOf rdf:resource="https://rsna.org/roadmap/ROADMAP_03561"/>
        <rdfs:label xml:lang="en">actual medical record number</rdfs:label>
        <skos:altLabel>actual MRN</skos:altLabel>
