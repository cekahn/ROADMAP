    <!-- https://rsna.org/roadmap/ROADMAP_03653 -->

    <owl:Class rdf:about="https://rsna.org/roadmap/ROADMAP_03653">
        <rdfs:subClassOf rdf:resource="https://rsna.org/roadmap/ROADMAP_03302"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://rsna.org/roadmap/ROADMAP_03648"/>
