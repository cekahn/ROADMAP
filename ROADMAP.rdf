    <!-- https://rsna.org/roadmap/ROADMAP_03356 -->

    <owl:Class rdf:about="https://rsna.org/roadmap/ROADMAP_03356">
        <rdfs:subClassOf rdf:resource="https://rsna.org/roadmap/ROADMAP_03357"/>
        <dc:source rdf:resource="https://doi.org/10.1002/9780470317082"/>
        <dc:source rdf:resource="https://doi.org/10.1117/1.JMI.11.2.024504"/>
        <rdfs:label xml:lang="en">negative likelihood ratio</rdfs:label>
        <rdfs:label xml:lang="es">razón de verosimilitud negativa</rdfs:label>
        <skos:altLabel xml:lang="en">LR-</skos:altLabel>
        <skos:altLabel xml:lang="en">NLR</skos:altLabel>
        <skos:altLabel xml:lang="en">negative LR</skos:altLabel>
        <skos:definition xml:lang="en">Negative Likelihood Ratio expresses how much less likely a negative test result is in individuals with the condition compared to those without it. It is calculated as the ratio of one minus sensitivity to specificity. Lower values indicate stronger evidence for the absence of the condition.

LR- = (1-sensitivity) / specificity</skos:definition>
    </owl:Class>
    


    <!-- https://rsna.org/roadmap/ROADMAP_03357 -->

    <owl:Class rdf:about="https://rsna.org/roadmap/ROADMAP_03357">
        <rdfs:subClassOf rdf:resource="https://rsna.org/roadmap/ROADMAP_03302"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://rsna.org/roadmap/ROADMAP_03648"/>
                <owl:someValuesFrom rdf:resource="https://rsna.org/roadmap/ROADMAP_04058"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source rdf:resource="https://doi.org/10.1002/9780470317082"/>
        <rdfs:label xml:lang="en">likelihood ratio</rdfs:label>
        <rdfs:label xml:lang="es">razón de verosimilitud</rdfs:label>
        <skos:altLabel xml:lang="en">LR</skos:altLabel>
        <skos:definition xml:lang="en">Likelihood ratio quantifies how much a diagnostic test result changes the odds of having a condition. It compares the probability of a given test result in individuals with the condition to the probability of the same result in those without it. This measure is independent of disease prevalence and can be computed for both positive and negative results.</skos:definition>
    </owl:Class>
    


