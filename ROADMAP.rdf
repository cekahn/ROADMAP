        <rdfs:label xml:lang="en">readability performance</rdfs:label>
        <skos:altLabel xml:lang="en">readability</skos:altLabel>
        <skos:definition xml:lang="en">Readability measures the ease with which users can read a text; it may measure the level of education required to read the subject text.</skos:definition>
    </owl:Class>
    


    <!-- https://rsna.org/roadmap/ROADMAP_04058 -->

    <owl:Class rdf:about="https://rsna.org/roadmap/ROADMAP_04058">
