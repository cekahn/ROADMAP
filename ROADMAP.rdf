        <rdfs:label xml:lang="en">enhanced multi-modal concordance</rdfs:label>
        <skos:altLabel xml:lang="en">MMC+</skos:altLabel>
        <skos:altLabel xml:lang="en">enhanced multi-modal concordance index</skos:altLabel>
    </owl:Class>
    


    <!-- https://rsna.org/roadmap/ROADMAP_00191 -->

    <owl:Class rdf:about="https://rsna.org/roadmap/ROADMAP_00191">
        <rdfs:subClassOf rdf:resource="https://rsna.org/roadmap/ROADMAP_03342"/>
        <dc:source rdf:resource="https://doi.org/10.1038/s41592-023-02151-z"/>
        <dc:source rdf:resource="https://doi.org/10.1111/j.1469-8137.1912.tb05611.x"/>
        <rdfs:label xml:lang="es">intersección de máscara sobre unión &gt; 0</rdfs:label>
        <rdfs:label xml:lang="en">mask intersection over union &gt; 0</rdfs:label>
        <skos:altLabel>mask IoU &gt; 0</skos:altLabel>
        <skos:definition>Mask intersection over union &gt; 0 determines whether a predicted region shares at least one pixel with the reference region. It operates as a binary localization criterion, indicating only the presence or absence of spatial overlap. This metric provides an extremely permissive threshold for detection tasks where coarse positional matching is sufficient.</skos:definition>
        <roadmap:ROADMAP_03651 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</roadmap:ROADMAP_03651>
        <roadmap:ROADMAP_03652 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</roadmap:ROADMAP_03652>
    </owl:Class>
    


