        <rdfs:label xml:lang="en">information content</rdfs:label>
        <skos:altLabel xml:lang="en">information-theoretic metric</skos:altLabel>
        <skos:definition xml:lang="en">A metric, such as entropy, derived from information theory.</skos:definition>
    </owl:Class>
    


    <!-- https://rsna.org/roadmap/ROADMAP_04057 -->

