        <skos:altLabel xml:lang="es">número de series</skos:altLabel>
        <skos:altLabel xml:lang="es">número de series de imágenes</skos:altLabel>
        <skos:definition xml:lang="en">Number of imaging series. An examination comprises one or more imaging series, each of which has one or more images.</skos:definition>
    </owl:Class>
    


